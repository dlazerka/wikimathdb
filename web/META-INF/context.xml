<?xml version="1.0" encoding="UTF-8"?>
<Context path="/">
	<Resource
		name="jdbc/wikimathdb"
		auth="Container"
		type="javax.sql.DataSource"
		username="wikimath"
		password="wikimath"
		driverClassName="org.postgresql.Driver"
		url="jdbc:postgresql://localhost:5432/wikimath"
		maxActive="8"
		maxIdle="4"
		/>
</Context>
