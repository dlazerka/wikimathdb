<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl"
  href="http://www.w3.org/Math/XSL/pmathml.xsl"?>
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
<head> </head>
<body>
<math xmlns="http://www.w3.org/1998/Math/MathML" display="block">
  <mi>&#x2202;</mi>
	<mrow>
		<mi>x</mi>
		<mo>=</mo>
		<mfrac>
			<mrow>
				<mrow>
					<mo>-</mo>
					<mi>b</mi>
				</mrow>
				<mo>*</mo>
				<msqrt>
					<mrow>
						<msup>
							<mi>b</mi>
							<mn>2</mn>
						</msup>
						<mo>-</mo>
						<mrow>
							<mn>4</mn>
							<mo>*</mo>
							<mi>a</mi>
							<mo>*</mo>
							<mi>c</mi>
						</mrow>
					</mrow>
				</msqrt>
			</mrow>
			<mrow>
				<mn>2</mn>
				<mo>*</mo>
				<mi>a</mi>
			</mrow>
		</mfrac>
	</mrow>
</math>
</body></html>
